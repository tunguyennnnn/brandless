type Product implements Node {
  id: ID!
  productId: String!
  name: String!
  link: String!
  brand: Brand @goField(forceResolver: true)
  brandId: ID!
  images: [String!]
  createdAt: Date!
}

type ProductConnection implements Connection {
  totalCount: Int!
  pageInfo: PageInfo!
  edges: [ProductEdge]
}

type ProductEdge implements Edge {
  node: Product
  cursor: Cursor!
}

extend type Query {
  products(limit: Int, cursor: Date): ProductConnection!
    @goField(forceResolver: true)
}
